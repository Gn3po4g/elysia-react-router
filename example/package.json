{
	"name": "elysia-react-router",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"dev": "bun --watch ./server/index.ts",
		"format": "prettier --ignore-path .gitignore --ignore-unknown --cache --cache-location node_modules/.cache/prettiercache --write .",
		"lint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern --cache --cache-location node_modules/.cache/eslint --fix .",
		"start": "NODE_ENV=production bun ./server/index.ts"
	},
	"dependencies": {
		"@react-router/fs-routes": "^7.6.0",
		"@tailwindcss/vite": "^4.1.6",
		"elysia": "^1.3.1",
		"elysia-react-router": "^0.3.0",
		"isbot": "^5.1.28",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.6.0",
		"tailwindcss": "^4.1.6"
	},
	"devDependencies": {
		"@react-router/dev": "^7.6.0",
		"@remix-run/eslint-config": "^2.16.6",
		"@types/react": "^19.1.4",
		"@types/react-dom": "^19.1.5",
		"eslint": "^9.26.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vite-tsconfig-paths": "^5.1.4"
	}
}
